!function(){window.klondike_is_mobile=window.matchMedia("(max-width: 1364px)").matches;let e=!1,t=!1;function o(){e=!0,jQuery.getScript("/media/assets/slideout/slideout.min.js",function(){jQuery("#sidebar-mobile").attr("style","");var e=new Slideout({panel:document.getElementById("wrapper"),menu:document.getElementById("slideout-preserved"),padding:370,tolerance:100,easing:"cubic-bezier(.32,2,.55,.27)",side:"left",touch:!1});e.disableTouch(),jQuery("#slideout-open").on("click",function(){jQuery(this).toggleClass("opened"),e.toggle(),t&&(jQuery("body").toggleClass("search-overlay"),jQuery("#search-form").fadeToggle("fast"),t=!1)})})}window.klondike_is_mobile&&o(),jQuery(window).on("resize",function(){window.matchMedia("(max-width: 1364px)").matches&&(e||o())}),jQuery("#side-menu > nav > ul > li.with-dropdown").on("click",function(e){e.preventDefault(),_this=this,jQuery("#side-menu > nav > ul > li.dropdown-opened").each(function(){_this!=this&&(jQuery(this).find(".dropdown").slideToggle("slow"),jQuery(this).removeClass("dropdown-opened"))}),jQuery(this).toggleClass("dropdown-opened"),jQuery(this).find(".dropdown").stop().slideToggle("slow")}),jQuery("#side-menu > nav > ul > li.with-dropdown > .dropdown").on("click",function(e){e.stopPropagation()});let n=setInterval(function(){chatBroInstance=jQuery("#chatbro"),chatBroStyleInstance=jQuery("head style:last-child:contains(chatbro_data_tippy)"),chatBroInstance.find(".chatbro_body")&&chatBroStyleInstance.length>0&&(jQuery("#chatbro").removeClass("chatbro_container"),chatBroStyleInstance.remove(),clearInterval(n),setTimeout(function(){jQuery("#chatbro").addClass("is-loaded"),setTimeout(function(){jQuery("#chatbro").addClass("is-loaded-finish")},1e3)},500))},100);jQuery("#mobile-search").on("click",function(){t?t=!1:(t=!0,jQuery("#search-form input").focus()),jQuery("body").toggleClass("search-overlay"),jQuery("#search-form").stop().fadeToggle("fast",function(){t||jQuery("#search-form").attr("style","")})}),jQuery("#paginat