jQuery.getScript("/media/assets/owl/owl.carousel.min.js",function(){jQuery("#material-images").owlCarousel({items:1,loop:!0,autoplay:!0,autoplayTimeout:4e3,autoplayHoverPause:!0,autoplaySpeed:1e3,animateOut:"fadeOut",mouseDrag:!1,touchDrag:!1,pullDrag:!1,freeDrag:!1}),jQuery("#related").owlCarousel({items:1,loop:!0,autoplay:!0,autoplayTimeout:3e3,autoplayHoverPause:!0,autoplaySpeed:500,margin:30})}),jQuery(".smooth-scroll").on("click",function(e){e.preventDefault(),jQuery([document.documentElement,document.body]).stop().animate({scrollTop:jQuery(jQuery(this).attr("href")).offset().top-150},1e3)});